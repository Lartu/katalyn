def isspace;
    in $char: $_[1];
    print($_);
    if len($char);
        if $char :: " \t\n";
            return 1;
        ok;
    ok;
    return 0;
ok;

def array;
    return $_;
ok;

def in_array;
    in $needle: $_[1];
    in $haystack: $_[2];
    in $i: 1;
    while $haystack[$i];
        if $haystack[$i] = $needle;
            return $i;
        ok;
        in $i: $i + 1;
    ok;
    return 0;
ok;

def ceil;
    return floor($_[1]) + 1;
ok;